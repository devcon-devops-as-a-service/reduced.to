{
	"name": "url-shortener",
	"version": "1.0.0",
	"description": "URL shortener is a web application that reduces the length of link URL. So it's easier to remember, share and track.",
	"scripts": {
		"build:all": "yarn build:frontend && yarn build:backend && ncp ./backend/dist ./dist",
		"build:frontend": "yarn --cwd ./frontend run build:ssg",
		"build:backend": "yarn --cwd ./backend run build",
		"postinstall": "yarn install:all && yarn build:all",
		"install:all": "yarn install:frontend && yarn install:backend",
		"install:frontend": "cd ./frontend && yarn",
		"install:backend": "cd ./backend && yarn",
		"start:prod": "node dist/main"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/origranot/url-shortener.git"
	},
	"author": "OriG",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/origranot/url-shortener/issues"
	},
	"homepage": "https://github.com/origranot/url-shortener#readme",
	"dependencies": {
		"@nestjs/common": "^9.1.4",
		"@nestjs/core": "^9.1.4",
		"@nestjs/platform-express": "^9.1.4",
		"cache-manager": "^5.0.0",
		"ncp": "^2.0.0"
	}
}
